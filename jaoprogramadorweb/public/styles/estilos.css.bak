 p,pre,em,blockquote,td{
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-style: normal;
	font-weight: bold;
	color: #0000FF;
 }

 h2{
 	text-align:left;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	color: #0000FF;
	text-align:center;
	/*background: url(../imagenes/coltabladeBorde.gif) repeat-x left ;		
	
	background-image:url("../imagenes/bordeListaRB.gif");
	background-repeat:no-repeat;*/
 }
 #cabeceraTabla table tr{
 	background:url("../imagenes/bordeListaRB.gif") left no-repeat;
 	background-repeat:no-repeat;
 
 }
 #cabeceraTabla table td{
 	background-image:url("../imagenes/coltabladeBorde.gif");
 	background-repeat:repeat-x;
 
 }
 h3{
	font-family: Verdana, Arial, Helvetica, sans-serif;
	
	font-style: italic;
	font-weight: bold;
	color: #0000CC;
	text-decoration: underline;
 }
 a{
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-style: normal;
	font-weight: bold;
	color: #0000FF;
	text-align:center;	
	text-decoration:none;
	
 }
a:link {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	font-style: normal;
	font-weight: bold;
	color: #0000FF;
}
a:visited {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	font-style: normal;
	font-weight: bold;
	color: #0000FF;
}
a:hover {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-style: normal;
	font-weight: bold;
	color: #0000FF;
}
